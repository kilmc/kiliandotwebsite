{
  "name": "kiliandotwebsite",
  "version": "0.1.0",
  "description": "The personal website of Kilian McMahon (me)",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass --watch site/scss:dist/css",
    "build:sass": "sass site/scss:dist/css",
    "watch:eleventy": "eleventy --serve",
    "build:eleventy": "eleventy",
    "start": "npm-run-all build:sass --parallel watch:*",
    "build": "npm-run-all build:*",
    "test": "jest --watch",
    "debug": "DEBUG=Eleventy* eleventy"
  },
  "engines": {
    "node": ">=14.x.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kilmc/kiliandotwebsite.git"
  },
  "author": "Kilian McMahon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kilmc/kiliandotwebsite/issues"
  },
  "homepage": "https://github.com/kilmc/kiliandotwebsite#readme",
  "devDependencies": {
    "jest": "^27.0.3"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "sass": "^1.34.0"
  }
}
