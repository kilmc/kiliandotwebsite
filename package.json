		"@typescript-eslint/parser": "^6.7.2",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"mdsvex": "^0.11.0",
		"postcss": "^8.4.30",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.4.1",
		"sass": "^1.68.0",
